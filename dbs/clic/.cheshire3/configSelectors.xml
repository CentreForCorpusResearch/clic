<c3:config xmlns:c3="http://www.cheshire3.org/schemas/config/" xmlns:mets="http://www.loc.gov/METS/" xmlns:dcterms="http://purl.org/dc/terms/" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:dc="http://purl.org/dc/elements/1.1/">
  <c3:subConfigs>
    <c3:subConfig type="selector" id="idSelector">
      <c3:docs>Record identifier attribute Selector</c3:docs>
      <c3:objectType>cheshire3.selector.MetadataSelector</c3:objectType>
      <c3:source>
        <c3:location type="attribute">id</c3:location>
      </c3:source>
    </c3:subConfig>
    <c3:subConfig type="selector" id="nowTimeSelector">
      <c3:docs>Current time function Selector</c3:docs>
      <c3:objectType>cheshire3.selector.MetadataSelector</c3:objectType>
      <c3:source>
        <c3:location type="function">now()</c3:location>
      </c3:source>
    </c3:subConfig>
    <c3:subConfig type="selector" id="anywhereXPathSelector">
      <c3:docs>Anywhere XPath Selector. Select all mets:xmlData nodes.</c3:docs>
      <c3:objectType>cheshire3.selector.XPathSelector</c3:objectType>
      <c3:source>
        <c3:location type="xpath">//mets:xmlData</c3:location>
      </c3:source>
    </c3:subConfig>
    <c3:subConfig type="selector" id="titleXPathSelector">
      <c3:docs>Title XPath Selector. Select from a number of potential locations.</c3:docs>
      <c3:objectType>cheshire3.selector.XPathSelector</c3:objectType>
      <c3:source>
        <!--METS wrapped docutils-native XML (e.g. from reStructured Text)-->
        <c3:location type="xpath">//mets:FContent/mets:xmlData/document/@title</c3:location>
        <!--Generic Dublin-Core terms-->
        <c3:location type="xpath">//dcterms:title</c3:location>
        <!--Generic Dublin-Core elements-->
        <c3:location type="xpath">//dc:title</c3:location>
        <!--METS wrapped file @LABEL-->
        <c3:location type="xpath">/mets:mets/@LABEL</c3:location>
      </c3:source>
    </c3:subConfig>
  </c3:subConfigs>
</c3:config>
